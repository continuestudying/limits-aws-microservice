{
	"AWSEBDockerrunVersion": 2,
	"containerDefinitions": [
		{
			"name": "config",
			"image": "continuestudying/limits-config",
			"hostname": "config",
			"essential": true,
			"memory": 256
		},
		{
			"name": "eureka",
			"image": "continuestudying/limits-eureka",
			"hostname": "eureka",
			"essential": false,
			"memory": 256,
			"portMappings": [
				{
					"hostPort": 8761,
					"containerPort": 8761
				}
			]
		},	
		{
			"name": "limits",
			"image": "continuestudying/limits-limits",
			"hostname": "limits",
			"essential": false,
			"memory": 256,
			"portMappings": [
				{
					"hostPort": 8080,
					"containerPort": 8080
				}
			]
		}
	]
}