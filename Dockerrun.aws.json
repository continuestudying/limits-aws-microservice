{
	"AWSEBDockerrunVersion": 2,
	"containerDefinitions": [
		{
			"name": "config",
			"image": "continuestudying/limits-config",
			"hostname": "config",
			"essential": false,
			"memory": 128
		},		
		{
			"name": "limits",
			"image": "continuestudying/limits-limits",
			"hostname": "limits",
			"essential": true,
			"memory": 128,
			"portMappings": [
				{
					"hostPort": 8080,
					"containerPort": 8080
				}
			]
		}
	]
}